        <servlet-name>UpdateEventServlet</servlet-name>
        <url-pattern>/UpdateEventServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>OrganizerServlet</servlet-name>
        <url-pattern>/OrganizerServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AvailableEventsServlet</servlet-name>
        <url-pattern>/AvailableEventsServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BookTicketServlet</servlet-name>
