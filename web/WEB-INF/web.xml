        <servlet-class>com.momentus.util.ConfirmBookingServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MyBookingsServlet</servlet-name>
        <servlet-class>com.momentus.util.MyBookingsServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CancelBookingServlet</servlet-name>
        <servlet-class>com.momentus.util.CancelBookingServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BookEventServlet</servlet-name>
        <servlet-class>com.momentus.util.BookEventServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteBookingServlet</servlet-name>
        <servlet-class>com.momentus.util.DeleteBookingServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>signup</servlet-name>
        <url-pattern>/signup</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegisterServlet</servlet-name>
        <url-pattern>/RegisterServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EventServlet</servlet-name>
        <url-pattern>/EventServlet</url-pattern>
    </servlet-mapping>
